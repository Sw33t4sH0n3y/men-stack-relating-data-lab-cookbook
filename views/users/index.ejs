<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Family</title>
  </head>
  <body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>Welcome Vibe Tribe Family</h1>

    <% if (users.length > 0) { %>
      <ul>
        <% users.forEach(userProfile => { %>
          <li>
        <a href="/users/<%= userProfile._id %>">
          <%= userProfile.username %>
        </a>
    </li>
  <% }) %>
</ul>
<% } else { %>
    <p>No users found</p>
    <% } %>
  </body>
  </html>    

